<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<TableView fx:controller="com.kryszak.controllers.FileTableViewController" fx:id="fileView" prefHeight="200.0"
           prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.112"
           xmlns:fx="http://javafx.com/fxml/1">
    <columns>
        <TableColumn fx:id="fileNameColumn" prefWidth="100.0" text="%fileName"/>
        <TableColumn fx:id="fileSizeColumn" prefWidth="100.0" text="%fileSize"/>
        <TableColumn fx:id="createdOnColumn" prefWidth="122.0" text="%createdOn"/>
    </columns>
</TableView>